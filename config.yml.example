plugins:
  - tableau_hll

settings:
  log-level: debug
  intercept-log: /home/grandrew/sandbox2/etable/postgresql-proxy/intercept.log
  general-log: /home/grandrew/sandbox2/etable/postgresql-proxy/general.log

instances:
  - listen:
      name: proxy
      host: 127.0.0.1
      port: 5112
    redirect:
      name: postgresql
      host: 127.0.0.1
      port: 5432
    intercept:
      commands:
        queries:
          - plugin: tableau_hll
            function: rewrite_query
        connects:
      responses:
